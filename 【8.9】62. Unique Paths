   //独一无二的路径问题。
   //思考：动态规划，首先初始化，对于边界值都设成1，因为他们只能来自一个方向，而其他块都是来自两个方向，有map[i][j]=map[i-1][j]+map[i][j-1];
   //特别注意，第三个循坏的起始值均为1！！边界已经计算过了。
   
   int uniquePaths(int m, int n) {
        
	    int a[100][100];  
        for (int i = 0; i < m; i++) {  
            a[i][0] = 1;  
        }  
        for (int i = 0; i < n; i++) {  
            a[0][i] = 1;  
        }  
        for (int i = 1; i < m; i++) {  
            for (int j = 1; j < n; j++) {  
                a[i][j] = a[i-1][j] + a[i][j-1];  
            }  
        }  
        return a[m-1][n-1];  
    }
